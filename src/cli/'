use structopt::StructOpt;

#[derive(StructOpt, Debug)]
#[structopt(name = "command")]
pub enum Command {
    #[structopt(name = "quote")]
    Quote {
        keywords: String,
        #[structopt(short, long)]
        verbose: bool,
    },
    #[structopt(name = "gif")]
    Gif {
        keywords: String,
        #[structopt(short, long)]
        verbose: bool,
    },
}
